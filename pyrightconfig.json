{
    "include": [
        "src"
    ],
    "exclude": [
        "**/node_modules",
        "**/build/**",
        "**/dist/**",
        "**/.venv/**",
        "**/.ruff_cache/**",
        "**/__pycache__",
        "src/typestubs"
    ],
    "ignore": [],
    "stubPath": "/home/elyo/workspace/python/__stubs__/",
    "extraPaths": [
        "src",
        "plugins",
        "convert/examples/clients/clientA/code/",
        "convert/examples/clients/clientB/code/",
        "convert/examples/clients/clientC/code/"
    ],
    "defineConstant": {
        "DEBUG": true
    },
    "reportMissingImports": "error",
    "useLibraryCodeForTypes": true,
    "executionEnvironments": [
        {
            "root": "src",
            "extraPaths": [
                "plugins",
            ]
        }
        {
            "root": "plugins",
            "extraPaths": [
                "src"
            ]
        },
        {
            "root": "convert/examples/clients/clientA/code/",
        },
        {
            "root": "convert/examples/clients/clientB/code/",
        },
        {
            "root": "convert/examples/clients/clientC/code/",
        },
    ]
}
